@* ReSharper disable once CheckNamespace *@
@namespace Comp
@inherits BasePagedComponent<IPartPropertiesViewModel, IProperty>

@code {
#nullable enable

  private int m_partId;

  /// <summary>
  /// Parent part id
  /// </summary>
